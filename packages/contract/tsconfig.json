	"compilerOptions": {
		"target": "es2020",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "build",
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true
	},
	"include": ["artifacts/**/*.json", "typechain-types"],
